# VITE_MAX_WORKSPACE_URL=https://workspace.maxplatform.com
# VITE_MAX_APA_URL=https://apa.maxplatform.com
# VITE_MAX_MLOPS_URL=https://mlops.maxplatform.com
# VITE_MAX_QUERYHUB_URL=https://queryhub.maxplatform.com
# VITE_MAX_LLM_URL=https://llm.maxplatform.com
# VITE_OLLAMA_BASE_URL=https://ollama.maxplatform.com
